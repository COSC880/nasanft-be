{
  "name": "nasanft-be",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "setup": "npm install && npm run generateTypes && npm run build",
    "generateTypes": "mkdirp src/types && dotenv \"cross-var supabase gen types typescript --project-id \"%SUPABASE_PROJECT_ID%\" > src/types/dbTypes.ts\"",
    "prestart": "npm run build",
    "start": "dotenv \"cross-var node ./dist/%NODE_ENV%/bin/www\"",
    "prebuild": "dotenv \"cross-var echo building in %NODE_ENV% mode && npm run generateTypes\"",
    "build": "dotenv \"cross-var tsc --build tsconfig.%NODE_ENV%.json\"",
    "postbuild": "dotenv \"cross-var copyfiles -u1 \"src/static/**\" \"dist/%NODE_ENV%\"\"",
    "test": "jest --coverage"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {"tsconfig": "tsconfig.development.json"}
      ]
    }
  },
  "dependencies": {
    "canvas": "^2.11.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "~1.9.1",
    "supabase": "^1.41.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.14.1",
    "@types/pixelmatch": "^5.2.4",
    "@types/pngjs": "^6.0.1",
    "@types/supertest": "^2.0.12",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^7.0.0",
    "jest": "^29.4.3",
    "mkdirp": "^2.1.4",
    "pixelmatch": "^5.3.0",
    "pngjs": "^7.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
